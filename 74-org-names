public List<String> orgNames(List<String> adjectives, List<String> animals) {
    List<String> lsCombinedNames = new List<String>();
    for (String sCurrentAdjective : adjectives){
        for (String sCurrentAnimal : animals){
            lsCombinedNames.Add (sCurrentAdjective+'-'+sCurrentAnimal);
        }
    }
    return lsCombinedNames;
}