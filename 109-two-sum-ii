public List<Integer> twoSum(List<Integer> nums, Integer target){
    Integer index = 0;
    Boolean bFound = false;
    List<Integer> lstIndexResult = new List<Integer>();
    
    while (index < nums.size() && !bFound) {
        Integer iCurrent = nums.get(index);
        Integer iComplement = target-iCurrent;
        if (nums.contains(iComplement)) {
            lstIndexResult.add(index);
            lstIndexResult.add(nums.indexof(iComplement));
            bFound = true;
        }
        index++;
    }

    return lstIndexResult ;
}