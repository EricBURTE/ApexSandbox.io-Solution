public Id insertCourse(String name, String details, Integer credits) {
    if (credits <1 || credits>4) {
        return null;
    }
    else{
        apxio__Course__c acCourse = new apxio__Course__c (Name = name, apxio__Course_Details__c = details, apxio__Credits__c = credits);
        insert acCourse;
        return acCourse.Id;
    }
}