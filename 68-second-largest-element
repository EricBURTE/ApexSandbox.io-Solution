public Integer secondLargest(List<Integer> numbers) {
    Integer iMaximum = Integer.MIN_VALUE;
    Integer iSecondBest = Integer.MIN_VALUE;
    for (Integer i : numbers) {
        if (i>iMaximum) {
            iMaximum = i;
        }
    }
    for (Integer j : numbers) {
        if (j>iSecondBest && j!=iMaximum) {
            iSecondBest = j;
        }
    }
    return iSecondBest;
}